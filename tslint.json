{
	"extends": "tslint-config-airbnb",
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"triple-equals": true,
		"no-trailing-whitespace": true,
		"trailing-comma": [
			false
		],
		"ter-indent": [true],
		"indent": [true, "spaces", 4]
	}
}