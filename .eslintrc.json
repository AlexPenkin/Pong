{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": ["warn", 4],
        "linebreak-style": "off",
        "no-mixed-operators": ["error", {"allowSamePrecedence": true}]
    }
}